{
    "name": "laravie/dynamodb",
    "description": "Eloquent syntax for DynamoDB",
    "keywords": ["laravel", "dynamodb", "aws"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bao Pham",
            "email": "gbaopham@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BaoPham\\DynamoDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BaoPham\\DynamoDb\\Tests\\": "tests/"
        }
    },
    "require": {
        "aws/aws-sdk-php": "^3.0",
        "illuminate/database": "^5.8",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8.5"
    },
    "scripts": {
        "test": "phpunit",
        "dynamodb": "java -Djava.library.path=./DynamoDBLocal_lib -jar dynamodb_local/DynamoDBLocal.jar --port 3000"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BaoPham\\DynamoDb\\DynamoDbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
